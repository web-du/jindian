/**首页分类样式**/
.index-class .mui-segmented-control.mui-segmented-control-inverted.mui-segmented-control-vertical .mui-control-item, .mui-segmented-control.mui-segmented-control-inverted.mui-segmented-control-vertical .mui-control-item.mui-active{
	border:none;
	background-color: #ffffff;
	color: #333;
	line-height: 30px;
	width:1.34rem;
	height: 0.58rem;
	line-height: 0.58rem;
	margin: 0 auto;
	border-radius:0.3rem;
	margin-bottom: 0.3rem;
	margin-top: 0.3rem;
}
.index-class .mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active{
	background-color: #e15149;
	color: #ffffff;
}
.index-class .mui-col-xs-3{
	height: 100%;
	background-color: #f5f5f4;
	width:1.7rem;
}
.index-class .mui-col-xs-9{
	width:5.8rem;
}
.index-class .mui-table-view{
	display: flex;
	display: -webkit-flex;
	justify-content:space-between;
	flex-wrap: wrap;
	padding: 0 0.25rem;
}
.index-class .mui-table-view:before{
	display: none;
}
.index-class .mui-table-view:after{
	display: none;
}
.index-class .mui-table-view-cell{
	width:2.5rem;
	padding:0.15rem;
	-moz-box-shadow:2px 2px 3px #ddd;
	-webkit-box-shadow:2px 2px 3px #ddd;
	box-shadow:2px 2px 3px #ddd;
	border-radius: 0.1rem;
	display: flex;
	display: -webkit-flex;
	align-items: center;
	margin: 0.25rem 0rem;
}
.index-class .mui-table-view-cell:after{
	display: none;
}
.index-class .mui-table-view-cell>a:not(.mui-btn){
	margin: 0;
	padding: 0;
}
.index-class .mui-table-view-cell .box{
	display: flex;
	display: -webkit-flex;
	justify-content: flex-start;
}
.index-class .mui-table-view-cell .box img{
	width: 0.87rem;
	height: 1.1rem;
	margin-right: 0.15rem;
