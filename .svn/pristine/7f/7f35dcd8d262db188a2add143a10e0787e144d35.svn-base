/**首页样式**/
body{
	font-family: "微软雅黑";
	background-color: #fff;
}
.mui-content{
	background-color: #fff;
}
.index-title{
	color:#e15149;
}
.search {
    width: 100%;
    height: 44px;
    background-color: #50adef;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 999;
}
.search-wrap {
    height: 44px;
}
.content-wrap {
    width: 92%;
    margin: 0 auto;
}
.search-input-group {
    width: 85%;
    height: 30px;
    background-color: #1386d7;
    border-radius: 4px;
}
.search-input-group {
    padding: 0 10px;
}
.search .mui-icon {
    padding: 0;
}
.search .search-icon {
    color: rgba(255,255,255,.6);
}
.search-input-group a, .search-input-group input {
    display: block;
}
.search-input-group a, .search-input-group input {
    display: block;
}
.search-input-group input {
    width: 80%;
    height: 100%;
    margin-left: 10px;
    background-color: #1386d7;
    border: none;
    margin: 0;
    font-size: .32rem;
    color: #fff;
}
.cancel-btn {
    display: block;
    font-size: .32rem;
    color:#e15149;
}
#header #search-open{
	color: #e15149;
}
.swiper-container img{
	width: 100%;
	height: 3.2rem;
	display: block;
}
.swiper-pagination-bullet{
	background: #fff;
	opacity: 1;
}
.swiper-pagination-bullet-active{
	opacity: 1;
    background: #e15149;
}
.index-nav{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
	padding: 0.5rem 0;
}
.index-nav .box{
	width: 25%;
}
.index-nav .box img{
	width: 0.65rem;
	height: 0.65rem;
	display: block;
	margin: 0 auto;
}
.index-nav .box p{
	font-size: 0.28rem;
	color: #333333;
	text-align: center;
	line-height: 1;
	margin: 0.2rem 0 0;
}
.line{
	width: 100%;
	height: 0.2rem;
	background-color: #f0f0f0;
}
.index-select{
	padding: 0.5rem 0.25rem;
}
.index-stitle{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
}
.index-stitle .name{
	display: flex;
	display: -webkit-flex;
	align-items: center;
	justify-content: flex-start;
}
.index-stitle .name img{
	width: 0.36rem;
	margin-right: 0.2rem;
}
.index-stitle .name p{
	font-size: 0.36rem;
	color: #333333;
}
.index-stitle .name p#Timeover{
	font-size: 0.32rem;
	color: #e15149;
	margin-left: 0.25rem;
}
.index-stitle .name p#Timeover span{
	font-size: 0.22rem;
	color: #999999;
	display: inline-block;
	margin-left:0.05rem;
	margin-right: 0.05rem;
}
.index-stitle .more{
	display: flex;
	display: -webkit-box;
	justify-content: flex-end;
	align-items: center;
}
.index-stitle .more span{
	font-size: 0.28rem;
	color: #999999;
}
.index-stitle .more img{
	width: 0.11rem;
}
.index-select .book-box{
	display: flex;
	display: -webkit-flex;
	justify-content: flex-start;
	margin-top: 0.35rem;
}
.index-select .book-box .box{
	width: 1.55rem;
	margin-right: 0.26rem;
}
.index-select .book-box .box:last-child{
	margin-right: 0;
}
.index-select .book-box .box img{
	display: block;
	width: 1.55rem;
	height: 1.95rem;
	margin-bottom: 0.25rem;
}
.index-select .book-box .box p{
	font-size: 0.28rem;
	color: #333333;
	line-height: 1.3em;
	height: 2.6em;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp:2;
	overflow: hidden;
}
.index-sell{
	padding: 0.5rem 0.25rem 0.25rem;
}
.index-sell .sell-box{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	flex-wrap: wrap;
	margin-top: 0.35rem;
}
.index-sell .sell-box .box{
	width: 3.37rem;
	margin-bottom: 0.25rem;
	border-radius: 0.2rem;
	-moz-box-shadow:0px 2px 1px #DDDDDD;
	-webkit-box-shadow:0px 2px 1px #DDDDDD;
	box-shadow:0px 2px 1px #DDDDDD;
}
.index-sell .sell-box .box img{
	display: block;
	width: 3.37rem;
	height: 1.82rem;
}
.index-sell .sell-box .box h2{
	font-size: 0.28rem;
	color: #333333;
	height: 0.6rem;
	line-height: 0.6rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	padding-left: 0.15rem;
}
.index-free{
	padding: 0.5rem 0.25rem 0;
}
.index-free .free-box .box a{
	display: flex;
	display: -webkit-flex;
	justify-content:space-between;
}
.index-free .free-box .box img{
	display: block;
	width: 1.6rem;
	height: 2rem;
}
.index-free .free-box .box .details{
	width: 5rem;
	margin-left: 0.25rem;
}
.index-free .free-box .box .details .name{
	display: flex;
	display: -webkit-flex;
	justify-content: flex-start;
	align-items: center;
}
.index-free .free-box .box .details .name p{
	font-size: 0.3rem;
	color: #333333;
	max-width: 3rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.index-free .free-box .box{
	padding: 0.4rem 0;
	border-bottom: 1px solid #eee;
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
}
.index-free .free-box .box .details .name span{
	color: #fff;
	font-size: 0.2rem;
	padding: 0 0.08rem;
	line-height: 0.35rem;
	height: 0.3rem;
	background-color: #6dbb0a;
	border-radius: 0.5rem;
	margin-left: 0.2rem;
	min-width: 0.72rem;
	text-align:center;
	box-sizing:content-box;
	-webkit-box-sizing:content-box;
	-moz-box-sizing:content-box;
}
.index-free .free-box .box .details .name span.over{
	background-color: #e15149;
}
.index-free .free-box .box .details .show{
	display: flex;
	display: -webkit-flex;
	justify-content: flex-start;
	align-items: center;
	margin: 0.2rem 0;
	flex-wrap: wrap;
}
.index-free .free-box .box .details .show span{
	font-size: 0.2rem;
	color: #999999;
	position: relative;
	padding-right: 0.1rem;
	margin-right: 0.1rem;
	line-height: 1;
}
.index-free .free-box .box .details .show span:last-child{
	padding-right: 0;
	margin-right: 0;
}
.index-free .free-box .box .details .show span:last-child::after{
	display: none;
}
.index-free .free-box .box .details .show span::after{
	position: absolute;
	content: '';
	width: 3px;
	height: 3px;
	border-radius: 50%;
	background-color: #999999;
	right: 0;
	top: 50%;
	margin-top: -1.5px;
}
.index-free .free-box .box .details .text p{
	font-size: 0.24rem;
	color: #333333;
	line-height: 1.5em;
	height: 3em;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
