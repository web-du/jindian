	background-color: #e15149;
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	justify-content: space-between;
	align-items: center;
	padding:0.8rem 0.25rem 0;
}
.myheader-bg .member-img{
	display:flex;
	display: -webkit-flex;
	display: -moz-flex;
	justify-content: center;
	align-items: center;
}
.myheader-bg .member-img img{
	width: 1.52rem;
	height: 1.52rem;
	border-radius: 50%;
	margin-right: 0.25rem;
}
.myheader-bg .member-img h2{
	font-size: 0.32rem;
	color: #fff;
	font-weight: bold;
}
.myheader-bg .member-img p{
	font-size: 0.24rem;
	color: #fff;
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	align-items: center;
}
.myheader-bg .member-img p span{
	padding-right: 0.2rem;
	margin-right: 0.2rem;
	border-right: 1px solid #fff;
}
.myheader-bg .member-img p span:last-child{
	border-right: none;
}
.myheader-bg>button{
	width: 1.32rem;
	height: 0.58rem;
	margin: 0;
	padding: 0;
	background-color: transparent !important;
	border:1px solid #fff !important;
	border-color: #fff !important;
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	align-items: center;
	justify-content:center;
}
.myheader-bg>button a{
	width: 1.2rem;
	height: 0.5rem;
	line-height: 0.5rem;
	text-align: center;
	background-color: #fff;
	font-size: 0.28rem;
	color: #e15149;
	border-radius: 0.3rem;
	font-weight: normal;
}
.account-page{
	position: relative;
	padding-top: 2.6rem;
}
.account-page .message{
	position: absolute;
	width: 7rem;
	left:50%;
	margin-left:-3.5rem;
	top: -1.55rem;
	background-color: #ffffff;
	padding: 0.9rem 0.5rem 0.5rem;
	border-radius: 0.2rem;
}
.account-page .message>img{
	position: absolute;
	width: 1.52rem;
	height: 1.52rem;
	border-radius: 50%;
	left: 50%;
	margin-left: -0.76rem;
	top:-0.76rem;
}
.account-page .message>h2{
	font-size: 0.32rem;
	color: #333;
	text-align: center;
	line-height: 1;
	margin-bottom: 0.15rem;
}
.account-page .message>p{
	text-align: center;
	font-size: 0.24rem;
	color: #666;
	text-align: center;
	line-height: 1;
}
.account-page .message .nav-top{
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	align-items: center;
	justify-content: space-between;
	margin-top: 0.5rem;
}
.account-page .message .nav-top .box img{
	width: 0.7rem;
	height: 0.6rem;
	display: block;
	margin: 0 auto;
}
.account-page .message .nav-top .box h2{
	font-size: 0.3rem;
	color: #333333;
	text-align: center;
	line-height: 1;
	margin-top: 0.2rem;
	margin-bottom: 0.1rem;
}
.account-page .message .nav-top .box p{
	font-size: 0.22rem;
	color: #999;
	text-align: center;
	line-height: 1;
}
.account-page .message .nav-top .box:nth-child(2){
	width: 2.55rem;
}
.account-page .my-account{
	width: 7rem;
	margin: 0 auto;
	background-color: #fff;
	padding: 0 0.22rem 0;
	margin-bottom: 1rem;
	border-radius: 0.2rem;
}
.account-page .my-account .To{
	height: 1.2rem;
	border-bottom: 1px solid #eee;
	width:100%;
}
.account-page .my-account .To:last-child{
	border-bottom: none;
}
.account-page .my-account .To a{
	display:box;
	display:-webkit-box;
	display:-moz-box;
	display: -webkit-flex;
	display: -moz-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-align-items: center;
    align-items: center;

    -webkit-box-align: center;

	/* 09版 */
    -webkit-box-pack: space-between;

     /* 12版 */
    -webkit-justify-content: space-between;
    -moz-justify-content: space-between;
    -ms-justify-content: space-between;
    -o-justify-content: space-between;
    justify-content: space-between;
	height: 100%;
	width:100%;
}
.account-page .my-account .To a>img{
	width: 0.2rem;
	height: 0.38rem;
}
.account-page .my-account .To p{
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	align-items: center;
}
.account-page .my-account .To p img{
	width: 0.45rem;
	height: 0.45rem;
	margin-right: 0.2rem;
}
.account-page .my-account .To p span{
	font-size: 0.3rem;
	color: #333;
}
.member-page .my-grade{
	padding: 0.4rem 0.25rem 0.5rem;
	background-color: #fff;
}
.member-page .my-grade .integral{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
}
.member-page .my-grade .integral h2{
	font-size: 0.3rem;
	color: #333;
}
.member-page .my-grade .integral p{
	font-size: 0.24rem;
	color: #666;
}
.member-page .my-grade .real{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
}
.member-page .my-grade .real span{
	font-size: 0.28rem;
