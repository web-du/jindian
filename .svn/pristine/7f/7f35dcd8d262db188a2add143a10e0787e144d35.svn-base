}
.index-class .mui-table-view-cell .box p span{
	font-size: 0.24rem;
	color: #666666;
}
.index-class .mui-table-view-cell .box p span::after{
	display: inline-block;
	content: '/';
}
.index-class .mui-table-view-cell .box p span:last-child::after{
	display: none;
}
/**全部分类页**/
.myheader{
	width: 100%;
	height: 0.8rem;
	background-color: #e15149;
	position: relative;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 99;
}
.myheader a{
	position: absolute;
	left: 0.25rem;
	height: 100%;
	width: 0.5rem;
	line-height: 1;
	color: #fff;
}
.myheader a>img{
	width: 0.23rem;
	display: block;
	margin-top: 0.2rem;
}
.myheader a::before{
	position: absolute;
	top:0.1rem;
}
.myheader .fans-title{
	width: 100%;
	height: 0.8rem;
	padding-top: 0.1rem;
}
.myheader .fans-title h2{
	font-size: 0.36rem;
	color: #FFF;
	text-align: center;
	line-height: 1;
}
.myheader .fans-title span{
	font-size: 0.28rem;
	color: #dfdfdf;
	display: block;
	text-align: center;
	line-height: 1;
}
.myheader .tab{
	width: 1.85rem;
	height:0.8rem;
	line-height:0.8rem;
	margin: 0 auto;
	font-size: 0.32rem;
	color: #fff;
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
}
.myheader .tab span{
	position: relative;
}
.myheader .tab span.now::before{
	position: absolute;
	left:50%;
	margin-left: -50%;
	bottom:0.1rem;
	content: '';
	width: 100%;
	transform: scale(0.8,1);
	-webkit-transform: scale(0.8,1);
	-moz-transform: scale(0.8,1);
	-ms-transform: scale(0.8,1);
	height: 1px;
	background-color: #fff;
}
.myheader>h2.title{
	font-size: 0.36rem;
	color: #FFF;
	height:0.8rem;
	line-height: 0.8rem;
	text-align: center;
}
.myheader>button{
	position: absolute;
	right:0.25rem;
	top:0;
	width: 1rem;
	height: 0.8rem;
	line-height: 0.8rem;
	text-align: center;
	border:none;
	padding: 0;
	margin: 0;
	background-color: #e15149;
	font-size: 0.32rem;
	color: #fff;
}
.myheader>img{
	position: absolute;
	width: 0.45rem;
	height: 0.45rem;
	top:0.17rem;
	right:0.25rem;
	z-index: 99;
}
.myheader>.screen{
	position: absolute;
	right:0.25rem;
	top:0;
	height: 0.8rem;
	width: 1rem;
	display: flex;
	display: -webkit-flex;
	align-items: center;
	justify-content: flex-end;
}
.myheader>.screen img{
	width: 0.2rem;
	margin-right: 0.1rem;
}
.myheader>.screen span{
	font-size: 0.28rem;
	color: #fff;
}
.myheader>.new-all{
	position:absolute;
	height:100%;
	width:1rem;
	right:0.25rem;
	top:0;
	display: flex;
	display: -webkit-flex;
	display: -moz-flex;
	align-items: center;
	justify-content:flex-end;
}
.myheader>.new-all a{
	position:static;
	width:100%;
	height:auto;
	text-align:right;
}
.class-nav{
	display: flex;
	display: -webkit-flex;
	justify-content: flex-start;
	align-items: center;
	padding: 0 0.25rem;
	position: fixed;
	left: 0;
	top:0.8rem;
	width: 100%;
	height: 0.8rem;
	border-bottom: 1px solid #eee;
	z-index: 102;
}
.class-nav .box{
	width: 33.33%;
